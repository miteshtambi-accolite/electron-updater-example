{
  "name": "electron-updater-example",
  "version": "0.8.1",
  "main": "main.js",
  "description": "electron-updater example project",
  "author": "Matt Haggard",
  "scripts": {
    "dev": "cross-env NODE_ENV=development electron .",
    "build": "electron-builder build --win --publish never",
    "deploy": "electron-builder build --win --publish always"
  },
  "devDependencies": {
    "electron": "^7.1.8",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "cross-env": "^6.0.3",
    "electron-log": "^4.0.3",
    "electron-updater": "^4.2.0"
  },
  "build": {
    "appId": "com.github.miteshtambi-accolite.electronupdaterexample",
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/miteshtambi-accolite/electron-updater-example.git"
  }
}
